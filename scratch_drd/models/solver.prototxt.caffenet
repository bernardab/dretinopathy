net: "models/train_val.prototxt"
test_iter: 100
test_interval: 5000
# lr when starting from scratch should be high
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
# stepsize should also be high
stepsize: 320000
display: 50
max_iter: 10000000
momentum: 0.9
weight_decay: 0.0005
snapshot: 25000
snapshot_prefix: "models/"
solver_mode: GPU
 